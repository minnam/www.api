{
    "env": {
        "compiler": {
            "bit.envs/compilers/typescript@3.1.38": {
                "rawConfig": {
                    "tsconfig": {
                        "compilerOptions": {
                            "target": "ES5",
                            "module": "CommonJS"
                        }
                    }
                }
            }
        }
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm",
    "overrides": {
        "*": {
            "dependencies": {
                "@bit/www.api.mongo.driver": "-",
                "@types/mongodb": "-",
                "@types/node": "-",
                "fastify": "-",
                "mongodb": "-"
            }
        },
        "mongo/driver": {
            "peerDependencies": {
                "@types/mongodb": "^3.3.15",
                "mongodb": "^3.5.2"
            },
            "propagate": true
        },
        "mongo/controller": {
            "peerDependencies": {
                "@bit/www.api.mongo.driver": "^0.0.1",
                "@types/mongodb": "^3.3.15",
                "mongodb": "^3.5.2"
            },
            "propagate": true
        },
        "fastify/*": {
            "peerDependencies": {
                "@types/node": "^13.7.0",
                "fastify": "^2.11.0"
            },
            "propagate": true
        }
    }
}