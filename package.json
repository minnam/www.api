{
  "name": "www.api",
  "version": "1.0.0",
  "description": "WWW",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minnam/www.api.git"
  },
  "author": "Min Nam",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/minnam/www.api/issues"
  },
  "homepage": "https://github.com/minnam/www.api#readme",
  "dependencies": {
    "@bit/www.api.mongo.driver": "^0.0.1",
    "@types/mongodb": "^3.3.15",
    "@types/node": "^13.7.0",
    "fastify": "^2.11.0",
    "mongodb": "^3.5.2"
  },
  "bit": {
    "env": {
      "compiler": {
        "bit.envs/compilers/typescript@3.1.38": {
          "rawConfig": {
            "tsconfig": {
              "compilerOptions": {
                "target": "ES5",
                "module": "CommonJS"
              }
            }
          }
        }
      }
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm",
    "overrides": {
      "*": {
        "dependencies": {
          "@bit/www.api.mongo.driver": "-",
          "@types/mongodb": "-",
          "@types/node": "-",
          "fastify": "-",
          "mongodb": "-"
        }
      },
      "mongo/driver": {
        "peerDependencies": {
          "@types/mongodb": "^3.3.15",
          "mongodb": "^3.5.2"
        },
        "propagate": true
      },
      "mongo/controller": {
        "peerDependencies": {
          "@bit/www.api.mongo.driver": "^0.0.1",
          "@types/mongodb": "^3.3.15",
          "mongodb": "^3.5.2"
        },
        "propagate": true
      },
      "fastify/*": {
        "peerDependencies": {
          "@types/node": "^13.7.0",
          "fastify": "^2.11.0"
        },
        "propagate": true
      }
    }
  }
}
